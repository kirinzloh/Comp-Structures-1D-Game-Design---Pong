module pongClk (
    input clk,
    input rst,
    output outClk
  ) {
  
  .clk(clk) {
    .rst(rst){
      //timechange changeState;
      dff c_delay;
      }
  }
   
  always {
    c_delay.d = c_delay.q + 1;
    outClk = 0;
    if (c_delay.q >= 50000){
      outClk = 1;
      c_delay.d = 0;
    }
  }
}
